<!--
File used to describe the user profiles which can then be used in 
the collection file
-->

<xml>
	<profiles>
		<profile>
			<name>addAgent</name>
			<user>
				<target>newuser</target>
				<action>inject</action>
			</user>
			<user>
				<target>newusersoftphone</target>
				<action>inject</action>
			</user>
			<udp>
				<target>udp</target>
				<action>inject</action>
			</udp>
			<phone>
				<target>softphone</target>
				<action>inject</action>
			</phone>
			<phone>
				<target>loggedoutphone</target>
				<action>inject</action>
			</phone>
			<user>
				<target>updateudp</target>
				<action>update</action>
			</user>
			<user>
				<target>updatesoftphone</target>
				<action>update</action>
			</user>
			<user>
				<target>updatepeudp</target>
				<action>update</action>
			</user>
			<user>
				<target>updatepesoftphone</target>
				<action>update</action>
			</user>
			<appuser>
				<target>updateappuser</target>
				<action>update</action>
			</appuser>
			<udplogin>
				<target>udplogin</target>
				<action>inject</action>
			</udplogin>
			<agent>
				<target>agent</target>
				<action>update</action>
			</agent>
			<agent>
				<target>agentsoftphone</target>
				<action>update</action>
			</agent>
		</profile>
		<profile>
			<name>updateAgent</name>
			<user>
				<target>updateuser</target>
				<action>update</action>
			</user>
			<udplogin>
				<target>udplogin</target>
				<action>inject</action>
			</udplogin>
			<agent>
				<target>agent</target>
				<action>update</action>
			</agent>
		</profile>
	</profiles>
</xml>