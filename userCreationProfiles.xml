<!--
File used to describe the user profiles which can then be used in 
the collection file
-->

<xml>
	<profiles>
		<profile>
			<name>addAgent</name>
			<user>
				<target>newuser</target>
				<action>inject</action>
			</user>
			<udp>
				<target>udp</target>
				<action>inject</action>
			</udp>
			<phone>
				<target>loggedoutphone</target>
				<action>inject</action>
			</phone>
			<user>
				<target>updateudp</target>
				<action>update</action>
			</user>
			<user>
				<target>updatepe</target>
				<action>update</action>
			</user>
			<appuser>
				<target>updateappuserudp</target>
				<action>update</action>
			</appuser>
			<udplogin>
				<target>udplogin</target>
				<action>inject</action>
			</udplogin>
		</profile>
		<profile>
			<name>addRemoteAgent</name>
			<user>
				<target>newuser</target>
				<action>inject</action>
			</user>
			<phone>
				<target>softphone</target>
				<action>inject</action>
			</phone>
			<user>
				<target>updatesoftphone</target>
				<action>update</action>
			</user>
			<user>
				<target>updatepe</target>
				<action>update</action>
			</user>
			<appuser>
				<target>updateappuserphone</target>
				<action>update</action>
			</appuser>
		</profile>
		<profile>
			<name>updateAgent</name>
			<user>
				<target>updateuser</target>
				<action>update</action>
			</user>
			<udplogin>
				<target>udplogin</target>
				<action>inject</action>
			</udplogin>
		</profile>
	</profiles>
</xml>