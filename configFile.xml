<!--
Overal configuration file
-->

<xml>
	<config>
		<!--Misc-->
		<log4j>DEBUG</log4j><!--Logging level : ERROR,INFO,DEBUG-->
		<language>french</language><!--french,english-->
		<automaticreport>enable</automaticreport><!--enable,disable-->
		<storerequest>enable</storerequest><!--Keep in memory the already asked UUID-->
		<splitter>+</splitter>
		<csvsplitter>,</csvsplitter>
		<maxtaskthread>10</maxtaskthread><!--The number of task allowed to process in the same time-->
		<!--Smtp parameters-->
		<smtpemailport>25</smtpemailport>
		<smtpemailprotocol>smtp</smtpemailprotocol>
		<smtpemailserver>tbd</smtpemailserver>
		<smtpemail>tbd</smtpemail>
		<smtpemailpassword></smtpemailpassword>
		<smtpemailreceiver>alexandre.ratel@spie.com</smtpemailreceiver><!--One or more destinations separated by a ;-->
		<!--CUCM parameters-->
		<axlhost>127.0.0.1</axlhost><!--IP of the CUCM server-->
		<axlport>8443</axlport><!--Port to contact the CUCM server using AXL-->
		<axlversion>105</axlversion><!--85 or 105--><!--AXL version of the CUCM-->
		<axlusername>AXL_TOAD</axlusername><!--AXL user name to contact the CUCM server-->
		<axlpassword>password</axlpassword><!--Password of the AXL user in clear !!!!Warning : keep it secret!!!!-->
		<!--UCCX parameters-->
		<uccxhost>127.0.0.1</uccxhost><!--IP of the UCCX server-->
		<uccxport>8443</uccxport><!--Port to contact the UCCX server using REST-->
		<uccxversion>105</uccxversion><!--Version of the UCCX server-->
		<uccxusername>UCCX_TOAD</uccxusername><!--Username to contact the UCCX server-->
		<uccxpassword>password</uccxpassword><!--Password !!!!Warning : keep it secret!!!!-->
		<uccxrmcmuser>UCCXrmcm</uccxrmcmuser><!--CUCM rmcm user to update while creating a new UCCX agents phone-->
		<!--RISPORT parameters-->
		<rishost>10.0.1.172</rishost><!--IP of the CUCM server-->
		<risport>8443</risport><!--Port to contact the CUCM server using RISPORT-->
		<risusername>ris</risusername><!--RISPORT user name to contact the CUCM server-->
		<rispassword>******</rispassword><!--Password of the RISPORT user in clear !!!!Warning : keep it secret!!!!-->
		<rismaxphonerequest>500</rismaxphonerequest><!--Max Phone response per request-->
		<!--Provisioning Misc-->
		<nodidrange>19011000:19019999</nodidrange>
		<didpartition>SDA_P</didpartition>
		<nodidpartition>HSDA_P</nodidpartition>
		<maxnumchar>30</maxnumchar><!--Integer-->
		<maxuseridindex>999</maxuseridindex><!--The higher index allowed for naming userid-->
		<!--Mandatory variables template-->
		<addagentusercreationprofilename>addAgent</addagentusercreationprofilename><!--The creation profile to use for agent creation-->
		<updateagentusercreationprofilename>updateAgent</updateagentusercreationprofilename><!--The creation profile to use for agent update-->
		<agentidprefix>*1M*agent.agenttype+office.name</agentidprefix>
		<!--Web server parameters-->
		<webserverport>8092</webserverport>
		<!--Copy Log parameters-->
		<targetdirectory>C:\WEBPHP\TOAD\WebPortal\log</targetdirectory>
	</config>
</xml>
